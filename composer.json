{
    "name": "leaffilter/libphonenumber-wrapper",
    "description": "A static wrapper class for parsing & outputting phone numbers using libphonenumber",
    "require": {
        "php": ">=5.3.0",
        "giggsey/libphonenumber-for-php": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-4": {
          "LeafFilter\\Util\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "LeafFilter\\Util\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
